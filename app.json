{
  "expo": {
    "name": "GatePassClosure",
    "slug": "GatePassClosure",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/tvss_logo.jpg",
    "scheme": "gatepassapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/tvss_logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jasmine3.GatePassClosure",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/tvss_logo.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jasmine3.GatePassClosure",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7c0f379a-f3fa-464a-a74d-e90bf0dfc089"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/8c7c2bff-e2cc-4c39-9600-f83cbf06240f"
    },
    "owner": "jasmine3"
  }
}
